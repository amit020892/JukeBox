<UserControl x:Class="JukeBoxSolutions.Controls.LibraryLabel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:JukeBoxSolutions.Controls"
             mc:Ignorable="d">
    <Grid>
        <Label x:Name="lblTitle" Content="Label" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Top" Foreground="#3F000000" FontWeight="Bold" FontSize="24" Background="{x:Null}"/>
        <Label x:Name="lblTitle2" Content="Label" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Top" Foreground="#3FFFFFFF" Background="{x:Null}" FontSize="24"/>
    </Grid>
</UserControl>
