<UserControl x:Class="JukeBoxSolutions.Controls.WifiControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:JukeBoxSolutions.Controls"
      xmlns:Style1="clr-namespace:JukeBoxSolutions.Controls.Style1"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid MinHeight="450" MinWidth="825">
        <Style1:BGRectangleStyle1 HorizontalAlignment="Stretch" VerticalAlignment="Stretch" MinHeight="450" Width="825"/>
        <StackPanel x:Name="stackWifi" HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Vertical" Background="#BF000000"/>
    </Grid>
</UserControl>
