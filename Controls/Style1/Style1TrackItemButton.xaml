<UserControl x:Class="JukeBoxSolutions.Controls.Style1.Style1TrackItemButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:JukeBoxSolutions.Controls.Style1"
             mc:Ignorable="d" 
             d:DesignHeight="70" d:DesignWidth="650">
    <Grid>
        <ToggleButton Content="ToggleButton" Grid.Column="1" HorizontalAlignment="Stretch" Height="70" Margin="0,0,0,0" Style="{DynamicResource TrackItemButtonStyleAlpha}" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
